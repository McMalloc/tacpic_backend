<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.1//EN" "http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd">

<svg viewBox="0 0 <%= width %>mm <%= height %>mm" width="<%= width %>mm" height="<%= height %>mm"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:cc="http://creativecommons.org/ns#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xlink="http://www.w3.org/1999/xlink">

  <defs>
    <style>
      @font-face {
      font-family: tacpic_swell_braille;
      src: url(data:font/woff;charset=utf-8;base64,<%= font_data %>) format('woff');

      }

      /*force exact colors when printing PDF in chrome*/
      /*also force correct rasterization by setting opacity, this is a bug*/
      /*https://stackoverflow.com/questions/9562330/svg-pixelation-in-chrome/45911373#45911373*/
      svg {
      -webkit-print-color-adjust: exact;
      opacity: 0.99999;
      }

      .label-container {
      width: 100%;
      height: 100%;
      padding: 2mm;
      box-sizing: border-box;
      border: 1mm transparent solid;
      background-color: white!important;
      }

      .editor-ui {
        display: none;
      }

      .label-container.bordered {
      border: 1mm black solid;
      }

      .label-braille {
      line-height: 14mm;
      font-family: tacpic_swell_braille;
      position: absolute;
      width: 100%;
      top: 5mm; /*half font size*/
      white-space: pre-wrap;
      color: black;
      font-size: 10mm;
      box-sizing: border-box;
      overflow: visible;
      }

      .label-black {
      line-height: 14mm;
      font-size: 14pt;
      margin-top: -4mm;
      font-family: Arial, Helvetica, sans-serif;
      width: 100%;
      border: none;
      overflow: visible;
      padding: 0;
      resize: none;
      box-sizing: border-box;
      color: blue;
      background-color: transparent;
      }

      .key-label-braille {
      font-family: tacpic_swell_braille;
      white-space: pre-wrap;
      font-size: 10mm;
      display: block;
      user-select: none;
      overflow: visible;
      color: black;
      line-height: 12mm;
      }

      .key-label-black {
      font-size: 14pt;
      user-select: none;
      display: block;
      font-family: Arial, Helvetica, sans-serif;
      color: blue;
      }

      .label-black.page-numbers {
      position: absolute;
      top: -2mm;
      margin: 0;
      }

      .version-label {
      font-size: 3mm;
      fill: blue;
      }
    </style>
  </defs>

  <metadata>
    <rdf:RDF>
      <cc:Work
         rdf:about="">
        <dc:format>image/svg+xml</dc:format>
        <dc:type
           rdf:resource="http://purl.org/dc/dcmitype/StillImage" />
        <dc:title><%= title %></dc:title>
        <dc:contributor>
          <cc:Agent>
            <dc:title>
            <%= contributors %> 
            </dc:title>
          </cc:Agent>
        </dc:contributor>
        <dc:rights>
          <cc:Agent>
            <dc:title></dc:title>
          </cc:Agent>
        </dc:rights>
        <dc:creator>
          <cc:Agent>
            <dc:title></dc:title>
          </cc:Agent>
        </dc:creator>
        <dc:description><%= description %></dc:description>
        <dc:date><%= date %></dc:date>
        <cc:license
           rdf:resource="http://creativecommons.org/licenses/by/4.0/" />
      </cc:Work>
      <cc:License
         rdf:about="http://creativecommons.org/licenses/by/4.0/">
        <cc:permits
           rdf:resource="http://creativecommons.org/ns#Reproduction" />
        <cc:permits
           rdf:resource="http://creativecommons.org/ns#Distribution" />
        <cc:requires
           rdf:resource="http://creativecommons.org/ns#Notice" />
        <cc:requires
           rdf:resource="http://creativecommons.org/ns#Attribution" />
        <cc:permits
           rdf:resource="http://creativecommons.org/ns#DerivativeWorks" />
      </cc:License>
    </rdf:RDF>
  </metadata>

  <%= content %>

  <text class="version-label" y="<%= height - 2 %>mm" x="<%= width - 6 %>mm">v<%= version_id %></text>
</svg>