<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.1//EN" "http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd">

<svg viewBox="0 0 <%= width %>mm <%= height %>mm" width="<%= width %>mm" height="<%= height %>mm"
     xmlns="http://www.w3.org/2000/svg"
     xmlns:xlink="http://www.w3.org/1999/xlink">

  <defs>
    <style>
      @font-face {
      font-family: tacpic_swell_braille;
      src: url(data:font/woff;charset=utf-8;base64,<%= font_data %>) format('woff');

      }

      /*force exact colors when printing PDF in chrome*/
      /*also force correct rasterization by setting opacity, this is a bug*/
      /*https://stackoverflow.com/questions/9562330/svg-pixelation-in-chrome/45911373#45911373*/
      svg {
      -webkit-print-color-adjust: exact;
      opacity: 0.99999;
      }

      .label-container {
      width: 100%;
      height: 100%;
      padding: 2mm;
      box-sizing: border-box;
      border: 1mm transparent solid;
      background-color: white;
      }

      .label-container.bordered {
      border: 1mm black solid;
      }

      .label-braille {
      line-height: 12mm;
      font-family: tacpic_swell_braille;
      position: absolute;
      width: 100%;
      top: 5mm; /*half font size*/
      white-space: pre-wrap;
      color: black;
      font-size: 10mm;
      box-sizing: border-box;
      overflow: visible;
      }

      .label-black {
      line-height: 12mm;
      font-size: 14pt;
      margin-top: -4mm;
      font-family: Arial, Helvetica, sans-serif;
      width: 100%;
      border: none;
      overflow: visible;
      padding: 0;
      resize: none;
      box-sizing: border-box;
      color: blue;
      background-color: transparent;
      }

      .label-black.page-numbers {
      position: absolute;
      top: -2mm;
      margin: 0;
      }

      .version-label {
      font-size: 3mm;
      fill: blue;
      }
    </style>
  </defs>

  <%= content %>

  <text class="version-label" y="<%= height - 2 %>mm" x="<%= width - 6 %>mm">v<%= version_id %></text>
</svg>